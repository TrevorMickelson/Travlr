{
  "name": "travlr",
  "version": "1.0.0",
  "description": "Travlr Getaways",
  "author": "SNHU",
  "private": true,
  "scripts": {
    "start": "node ./bin/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "^4.18.2",
    "hbs": "^4.2.0",
    "http-errors": "~1.6.3",
    "mongoose": "^5.11.15",
    "morgan": "~1.9.1",
    "nodemon": "^3.0.1",
    "seedgoose": "^2.0.2"
  },
  "comment": {
    "note": "Use this command for seedgoose -> ./node_modules/.bin/seedgoose seed"
  },
  "seedgoose": {
    "modelBaseDirectory": "app_server/models",
    "models": [
      "**/*.js",
      "!db.js"
    ],
    "data": "data",
    "db": "mongodb://127.0.0.1:27017/travlr"
  }
}
